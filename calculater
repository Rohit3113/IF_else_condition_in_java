import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a= sc.nextInt();
		String button = sc.next();
		int b= sc.nextInt();
		switch(button){
		    case "+":System.out.print(a+b);
		    break;
		    case "-":System.out.print(a-b);
		    break;
		    case "*":System.out.print(a*b);
		    break;
		    case "/":System.out.print(a/b);
		    break;
		    default: System.out.print("invalid");
		}
		    
	}
}
